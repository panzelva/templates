{
  "name": "express-postgres-objection",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "core-js": "3",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.2",
    "objection": "^2.2.2",
    "pg": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.3",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier src/**/*.{js,ts} --list-different",
    "format:fix": "yarn format --write",
    "start": "nodemon --exec \"babel-node --extensions .ts src/start.ts\" --watch src --ext .ts",
    "build": "yarn build:clean && yarn build:types && yarn build:js",
    "build:clean": "rimraf ./dist",
    "build:types": "tsc --noEmit",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "db:schema:create": "babel-node --extensions .ts src/tools/createSchema.ts",
    "db:schema:drop": "babel-node --extensions .ts src/tools/dropSchema.ts",
    "db:migration:new": "knex migrate:make --migrations-directory src/database/migrations -x ts",
    "db:migration:run": "babel-node --extensions .ts src/tools/migrateDatabase.ts",
    "db:seed": "babel-node --extensions .ts src/tools/seedDatabase.ts",
    "db:dev": "yarn db:schema:drop && yarn db:schema:create && yarn db:migration:run && yarn db:seed"
  }
}
