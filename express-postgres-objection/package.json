{
  "name": "express-postgres-objection",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "dependencies": {
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "knex": "^0.21.2",
    "objection": "^2.2.2",
    "pg": "^8.3.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "@types/node": "^14.0.27",
    "@typescript-eslint/eslint-plugin": "^3.8.0",
    "@typescript-eslint/parser": "^3.8.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^4.0.8",
    "nodemon-webpack-plugin": "^4.3.1",
    "prettier": "^2.0.5",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^2.5.0"
  },
  "scripts": {
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "yarn lint --fix",
    "format": "prettier src/**/*.{js,ts} --list-different",
    "format:fix": "yarn format --write",
    "start": "webpack --config webpack.config.js --watch",
    "build": "webpack --config webpack.config.js -p",
    "prod": "node dist/server.js",
    "db:schema:create": "ts-node -P tsconfig.json tools/createSchema.ts",
    "db:schema:drop": "ts-node -P tsconfig.json tools/dropSchema.ts",
    "db:migration:new": "knex migrate:make --migrations-directory src/database/migrations -x ts",
    "db:migration:run": "ts-node -P tsconfig.json tools/migrateDatabase.ts",
    "db:seed": "ts-node -P tsconfig.json tools/seedDatabase.ts"
  }
}
